<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("/partials/head") %>
  </head>
  <body>
    <h1>Users</h1>

    <ul>
      <% users.map(user => { %>
      <li>
        <%= user.name %>
        <button
          onclick="DeletUser(this.dataset.id)"
          data-id="<%= user.id %>"
        
          type="button" > delete
        </button>
        <button type="button">update</button>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
